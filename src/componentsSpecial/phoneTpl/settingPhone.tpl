<div class="setting-panel">
  <a class="setting-panel-switch"></a>
  <div class="setting-panel-title">
       <div class="inner-title">组件设置</div>
  </div>
  <div class="setting-panel-content">
      <div class="DialogWindow" style="margin-bottom: 24px;">
          <div class="innerDialog" id="settingId" style="padding-bottom:0;height:120px;">
              <div id="show_pic_box" style="width:100%;height:100%;"></div>
        			<div class="groupSkin-content clearfix" style="text-align:center; margin-bottom: 15px;">
        				  <div id="file_upload" class="uploadify-button" style="float:left;margin-left:12px;margin-top:10px;cursor:pointer;line-height: 24px;margin-top:11px;color:#1c9ae3;">
        				      更换
        				  </div>
        			</div>
          </div>
      </div>
      <div class="innerDialog styleSetting" style="border-top:0px solid #ddd;" hasmodified="1">
          <div>
              <div class="advanceskin-content">
                <div>
                  <div class="groupSkin-content">
                    <div id="background_color" class="colorSelectorWrapper none" style="height: 50px; display: block;">
                      <label class="skin-label" style="float: left;margin-left:32px;">电话号码</label>
                      <div class="c-colorWrapper n-c-colorWrapper" style="float:left;margin:-26px 0 0 -8px;">
                        <input type="" class="phoneValue" name="">
                      </div>
                    </div>
                  </div>
                </div>
                <span class="line"></span>
                <div style="padding-bottom: 5px;padding-top: 30px;">
                  <div id="opacity" class="none" style="display: block;">
                    <!-- <div class="labelSkin-label">高级设置:</div> -->
                    <div class="groupSkin-content">
                      <label class="label_slide" style="margin-left:32px;">不透明度</label>
                      <div class="clearfix progress">
                        <p maxvalue="100" value="100">
                          <b class="opacity"><i class="progress-circle"></i></b>
                        </p>
                      </div>
                    </div>
                  </div>
                </div>
                <div>
                </div>
              </div>
          </div>
      </div>
  </div>
</div>